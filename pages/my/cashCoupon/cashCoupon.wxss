/* pages/my/cashCoupon/cashCoupon.wxss */
.list{
    width: 100%;
    position: relative;
    top: 0;
    left: 0;
}
.list image{
    width: 100%;
}
.list-content{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
}
.list-left{
    width: 70%;
    height: 100%;
    padding: 0 50rpx;
    color: #333336;
}
.list-right{
    width: 30%;
    height: 100%;
    font-size: 42rpx;
    color: #ff9c00;
}
.list-right .rmb{
    font-size: 28rpx;
    line-height: 42rpx;
}
.list-title{
    font-weight: bold;
}
.list-used{
    color: #979797;
}