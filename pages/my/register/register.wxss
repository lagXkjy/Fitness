/* pages/my/register/register.wxss */
.logo{
    width: 500rpx;
    display: block;
    margin: 70rpx auto 60rpx;
}
.list{
    height: 100rpx;
    border-bottom: 1px solid #f5f6f9;
}
.list>view:nth-child(1) {
    width: 23%;
    height: 100%;
}
.list>view:nth-child(2) {
    width: 77%;
    height: 100%;
}
input{
    width: 100%;
    height: 100%;
}
button {
    height: 90rpx;
    background: #ff425b;
    border-radius: 5rpx;
    margin-top: 50rpx;
}
.placeholder{
    color: #a6a6a6;
}
.right{
    width: 20rpx;
}