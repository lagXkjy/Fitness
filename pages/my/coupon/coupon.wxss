.list {
  height: 220rpx;
  border-radius: 10rpx;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
}

.select {
  background: #e84459;
  height: 50rpx;
  width: 110rpx;
  position: absolute;
  top: 0rpx;
  right: -50rpx;
  transform: rotateZ(45deg);
  transform-origin: 50% 0;
}

.select image {
  width: 28rpx;
  transform: rotateZ(-45deg);
  margin-bottom: 7rpx;
}

.list-bottom {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
}

.list-prompt {
  height: 60rpx;
}

.list-point-box {
  position: relative;
  left: 0;
  bottom: -3rpx;
}

.list-point {
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
}

.list-price {
  font-size: 50rpx;
  line-height: 50rpx;
}

.list-price-box {
  height: 50rpx;
  margin-right: 50rpx;
}

.list-content {
  width: 100%;
  height: 160rpx;
  padding-left: 50rpx;
  padding-right: 50rpx;
}

.list-bg1 {
  background: #a5deef;
}

.list-b1 {
  background: #57c3e3;
}

.list-bg2 {
  background: #ffa9b4;
}

.list-b2 {
  background: #ff7385;
}

.list-bg3 {
  background: #ffc67c;
}

.list-b3 {
  background: #ff9639;
}

.list-bg-overdue {
  background: #b7bbc2 !important;
}

.list-b-overdue {
  background: #a9acb3 !important;
}

.list-btn1 {
  background: #e4f5fa;
  color: #57c3e3;
}

.list-btn2 {
  background: #ffe5e9;
  color: #ff7385;
}

.list-btn3 {
  background: #ffeed8;
  color: #ff9639;
}

.list-btn {
  height: 38rpx;
  width: 120rpx;
  border-radius: 38rpx;
}
